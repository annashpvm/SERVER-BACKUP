SELECT SUM(ESI_AMOUNT) FROM HrdProcess.emp_monthly_salary_header
where sal_month=11 and sal_year=2018  and  ESI_AMOUNT

SELECT * FROM HrdProcess.emp_monthly_salary_header
where sal_month=11 and sal_year=2018  
and  employee_type='CONTRACT WORKER' and net_salary_rounded>15000 and pf_amount>0


SELECT * FROM HrdProcess.confirm_attendance
where month(attendance_date)=10   and  employee_type='CONTRACT WORKER' and employee_roll_no=42356

SELECT * FROM HrdProcess.emp_monthly_salary_header
where sal_month=11 and sal_year=2018  and  employee_roll_no=42586


SELECT * FROM HrdProcess.emp_monthly_salary_header
where sal_month=11 and sal_year=2018  and  
basic_salary+incentive+allowance+conveyance+contract_wages+
ot_wages+ot_incentive+vehicle_allowance-pf_amount-esi_amount-short_attendance_deduction<0


SELECT * FROM HrdProcess.hr_tbl_deductionDetails
where deduction_month=10 and deduction_year=2018  and employee_roll_no=32772